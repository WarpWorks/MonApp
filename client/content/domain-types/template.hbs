{{#partial "content"}}
  <div class="container" role="main">
    <div class="row">
      <div class="col-xs-12">
          Available entity types:
        <ol class="domain-types">
{{#each _embedded.entities}}
          <li>
              <a href="{{_links.self.href}}">{{type}}</a>
              {{#if isDefault}}<span title="Default Entity Type">*</span>{{/if}}
          </li>
{{/each}}
        </ol>
      </div>
    </div>
  </div>
{{/partial}}

{{> ../templates/page}}
