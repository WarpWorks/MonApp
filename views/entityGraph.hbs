<div class="container" role="main">
    <div id="mynetwork"></div>
    <form>
        Mode:
        <label class="radio-inline">
            <input type="radio" name="optradio" checked="checked" id="showAllModeI" />Show All
        </label>
        <label class="radio-inline">
            <input type="radio" name="optradio" id="entityClusterModeI" />Browse Entity Hierarchy
        </label>
    </form>
    <form>
        Show:
        <label class="checkbox-inline">
            <input type="checkbox" checked="checked" id="aggregationsI" />Aggregations
        </label>
