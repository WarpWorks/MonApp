<div class="p-item panel panel-default">
	<div class="panel-body">
		{{#each panelItems}}
			{{#if (isSeperator)}}
				{{> ./_seperatorPanel}}
			{{/if}}
			{{#if (isRelationshipPanel)}}
				{{> ./_relationshipPanel entity=this}}
			{{/if}}
			{{#if (isBasicProperty)}}
				{{> ./_basicPropertyPanel basicProperty=this}}
			{{/if}}
			{{#if this.enumPanelItems}}
				{{> ./_enumPanel}}
			{{/if}}
		{{/each}}
	</div>
</div>