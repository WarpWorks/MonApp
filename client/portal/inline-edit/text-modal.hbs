<div class="modal fade" role="dialog" tabindex="-1" aria-hidden="true"
  data-warpjs-modal="{{MODAL_NAME}}"
  >

  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <button type="button" class="close" data-dismiss="modal" arial-label="Close" aria-hidden="true">&times;</button>
        <div class="row">
          <div class="col-sm-4 warpjs-list-container">
            <div class="row warpjs-document-name">
              <div class="col-xs-12">
                <div class="warpjs-label">Document Name</div>
                <div class="warpjs-content"></div>
              </div>
            </div>

            <div class="row warpjs-document-elements">
              <div class="col-xs-12">
                <div class="warpjs-label">Document elements</div>
                <div class="warpjs-content"></div>
              </div>
            </div>
          </div>

          <div class="col-sm-8 warpjs-detail-container">
            <div class="warpjs-placeholder">
              &lt;--- Select an item to edit.
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>

</div>
