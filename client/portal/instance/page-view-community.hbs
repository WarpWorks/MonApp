{{#each panels as |panel|}}
  {{#if panel.showPanel}}
    <div class="row warpjs-page-view-side-panel warpjs-page-view-community">
      {{> page-view-community-section title="Authors" users=panel._embedded.authors}}
      {{> page-view-community-section title="Contributors" users=panel._embedded.contributors}}
    </div>
  {{/if}}
{{/each}}
