<div class="relationship-panel-item"
  data-relationship-model="{{relationship.model.name}}"
  data-warpjs-doc-level="{{relationship.docLevel}}"
  data-warpjs-url="{{relationship._links.self.href}}"
  data-warpjs-entity-type="{{relationship.model.entityType}}"
  >
{{#if (isCsvRelationship)}}
    {{> relationship-panel-item-csv canEdit=canEdit}}
{{else if (isTableRelationshipPanelItem)}}
    {{> relationship-panel-item-table}}
{{else if (isCarouselRelationshipPanelItem)}}
    {{> relationship-panel-item-carousel}}
{{else if (isPreviewRelationship)}}
    TODO: How to do Preview Relationship?
{{else}}
    TODO: Unknown relationship-panel-item style '{{style}}'.
{{/if}}
</div>
