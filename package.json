{
  "name": "@warp-works/warpjs",
  "version": "1.0.0-rc9",
  "description": "Content management system for WarpWorks in javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/WarpWorks/warpjs.git"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "Dirk Slama",
    "email": "dirk.slama@gmx.de"
  },
  "contributors": [
    {
      "name": "Hữu Ðà Trần",
      "email": "huuda.tran@quoininc.com",
      "url": "https://www.quoininc.com/staff/huu-da-tran/"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "coverage:integration": "nyc npm run -s test:integration",
    "coverage:unit": "nyc npm run -s test:unit",
    "coverage": "npm run -s coverage:unit && npm run -s coverage:integration",
    "test:integration": "mocha \"lib/**/*.integration.test.js\"",
    "test:unit": "mocha \"lib/**/*.unit.test.js\"",
    "test": "npm run -s test:unit && npm run -s test:integration",
    "lint:client": "eslint --fix --config=.eslintrc.client.json --ignore-pattern='*.test.js' public/app",
    "lint:node": "eslint --fix --ignore-pattern='*.test.js' server.js lib",
    "lint:test": "eslint --fix --config=.eslintrc.test.json \"lib/**/*.test.js\"",
    "lint": "npm run -s lint:node && npm run -s lint:test"
  },
  "dependencies": {
    "@quoin/expressjs-routes-info": "0.1.7",
    "@quoin/node-rc": "0.1.1",
    "@warp-works/core": "1.0.0-rc3",
    "debug": "2.6.8",
    "hal": "0.1.0",
    "hbs": "4.0.1",
    "hbs-utils": "0.0.4",
    "lodash": "4.17.4",
    "mongodb": "2.2.28",
    "serve-favicon": "2.4.3",
    "tinymce": "4.6.3"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "0.1.4",
    "@quoin/node-test-helpers": "0.2.1",
    "@warp-works/warpjs-mongo-persistence": "0.1.1",
    "body-parser": "1.17.2",
    "cookie-parser": "1.4.3",
    "eslint": "3.19.0",
    "eslint-plugin-html": "2.0.3",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "express": "4.15.3",
    "mocha": "3.4.2",
    "nyc": "11.0.2"
  },
  "peerDependencies": {
    "body-parser": ">=1.17.0",
    "cookie-parser": ">=1.4.0",
    "express": ">=4.0.0"
  }
