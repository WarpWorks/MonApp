<div class="warpjs-list-item-value {{#unless item.name}}warpjs-list-item-value-untitled{{/unless}}"
  data-warpjs-type="{{item.type}}"
  data-warpjs-id="{{item.id}}"
  data-warpjs-position="{{item.position}}"
  data-warpjs-name="{{item.name}}"
  data-warpjs-description="{{item.description}}"
  data-warpjs-level="{{item.level}}"
  data-warpjs-subdocuments="{{item.subDocuments}}"
  data-warpjs-reference-type="{{item.reference.type}}"
  data-warpjs-reference-id="{{item.reference.id}}"
  data-warpjs-reference-name="{{item.reference.name}}"
  data-warpjs-images="{{item.images}}"
  >
    {{#if item.name}}{{item.name}}{{else}}untitled{{/if}}
</div>

<div class="warpjs-list-item-status">
  <div class="warpjs-list-item-status-icon">&bull;</div>
</div>
