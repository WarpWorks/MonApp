
<div class="table-container">
    <table class="table table-hover" id="DefaultTableViewTable">
        <thead>
            <tr>
                
                <th> </th>
            </tr>
        </thead>
        <tbody id="HotSpot-DefaultTableView">
        <tbody>
    </table>
</div>
<script>
    HotSpot_updateDefaultTableView = function (relationship) {
            var queryResult = relationship.queryResults;
            queryResult.forEach (function (elem) {
                var TDs= "";
                $("#HotSpot-DefaultTableView").append("<tr class='HotSpot-DefaultTableView-row' id='"+elem.id+"'>"+TDs+"<td><span class='glyphicon glyphicon-forward'></span></td></tr>");
            });

            $(".HotSpot-DefaultTableView-row").click(function () {
                window.location.href = "./../app/HotSpot?oid="+$(this).attr('id');
            });
        }
</script>
