
<div class="table-container">
    <table class="table table-hover" id="DefaultTableViewTable">
        <thead>
            <tr>
                
                    <th>KPIs</th>
                
                <th> </th>
            </tr>
        </thead>
        <tbody id="UseCase-DefaultTableView">
        <tbody>
    </table>
</div>
<script>
    UseCase_updateDefaultTableView = function (relationship) {
            var queryResult = relationship.queryResults;
            queryResult.forEach (function (elem) {
                var TDs= "<td>"+elem.get_KPIs()+"</td>"+"";
                $("#UseCase-DefaultTableView").append("<tr class='UseCase-DefaultTableView-row' id='"+elem.id+"'>"+TDs+"<td><span class='glyphicon glyphicon-forward'></span></td></tr>");
            });

            $(".UseCase-DefaultTableView-row").click(function () {
                window.location.href = "./../app/UseCase?oid="+$(this).attr('id');
            });
        }
</script>
