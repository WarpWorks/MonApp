<div class="row warpjs-page-view-summary">
  <div class="warpjs-table">
    <div class="col-xs-4 warpjs-page-view-summary-results-title warpjs-cell">
      {{summary.[0].title}}
    </div>
    <div class="col-xs-4 warpjs-page-view-summary-challenge-title warpjs-cell">
      {{summary.[1].title}}
    </div>
    <div class="col-xs-4 warpjs-page-view-summary-solution-title warpjs-cell">
      {{summary.[2].title}}
    </div>
  </div>

  <div class="warpjs-table">
    <div class="col-xs-4 warpjs-page-view-summary-results-content warpjs-cell">
      {{{summary.[0].content}}}
    </div>

    <div class="col-xs-4 warpjs-page-view-summary-challenge-content warpjs-cell">
      {{{summary.[1].content}}}
    </div>

    <div class="col-xs-4 warpjs-page-view-summary-solution-content warpjs-cell">
      {{{summary.[2].content}}}
    </div>
  </div>
</div>
