  "main": "index.js",
  "scripts": {
    "check": "grunt check && npm run jscpd",
    "jscpd": "jscpd lib client server",
    "prepare": "grunt build",
    "pretest": "npm run check",
    "prepack": "npm run coverage",
    "coverage:integration": "nyc --report-dir=reports/coverage/integration npm run -s test:integration",
    "coverage:unit": "nyc --report-dir=reports/coverage/unit npm run -s test:unit",
    "coverage": "npm run -s coverage:unit && npm run -s coverage:integration",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "precoverage": "npm run check",
    "test:integration": "mocha \"lib/**/*.integration.test.js\" \"client/**/*.integration.test.js\" \"server/**/*.integration.test.js\"",
    "test:unit": "mocha \"lib/**/*.unit.test.js\" \"client/**/*.unit.test.js\" \"server/**/*.unit.test.js\"",
    "test": "npm run -s test:unit && npm run -s test:integration",
    "lint:client": "grunt eslint:client",
    "lint:node": "grunt eslint:node",
    "lint:test": "grunt eslint:test",
    "lint": "grunt eslint",
    "backup": "node ./scripts/backup-data.js",
    "build:dev": "DEBUG=W2:* NODE_ENV=development grunt",
    "dev": "DEBUG=W2:* npm start"
  },
  "bin": {
    "warpjs-backup": "./scripts/backup-data.js",
    "warpjs-reindex": "./scripts/reindex-data.js",
    "warpjs-import-domains-to-db": "./scripts/import-domains-to-db.js",
